{
    "name": "nyan.stream",
    "description": "Source code of NYAN.STREAM website.",
    "private": true,
    "version": "3.1.0",
    "author": "Dmitry <dmitry@cojam.ru> (https://e965.ru)",
    "repository": "github:nyanstream/nyan.stream",
    "license": "MIT",
    "scripts": {
        "start": "next",
        "build": "next build",
        "postbuild": "next-sitemap --config next-sitemap.config.js",
        "dist": "npm run build && next export",
        "lint": "next lint",
        "prettier": "prettier --write **/*.{json,js,ts,tsx,scss}",
        "lint-staged": "lint-staged",
        "prepare": "is-ci || husky install"
    },
    "lint-staged": {
        "*.{json,js,ts,tsx,scss}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "@next/font": "13.1.1",
        "clsx": "1.2.1",
        "dayjs": "1.11.7",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-icons": "4.7.1",
        "react-markdown": "8.0.4"
    },
    "devDependencies": {
        "@types/node": "18.11.18",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "5.48.0",
        "@typescript-eslint/parser": "5.48.0",
        "eslint": "8.31.0",
        "eslint-config-next": "13.1.1",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.3",
        "is-ci": "3.0.1",
        "lint-staged": "13.1.0",
        "next": "13.1.1",
        "next-sitemap": "3.1.44",
        "prettier": "2.8.1",
        "sass": "1.57.1",
        "typescript": "4.9.4"
    }
