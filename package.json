{
    "name": "nyan.stream",
    "description": "Source code of NYAN.STREAM website.",
    "private": true,
    "version": "3.1.0",
    "author": "Dmitry <dmitry@cojam.ru> (https://e965.ru)",
    "repository": "github:nyanstream/nyan.stream",
    "license": "MIT",
    "scripts": {
        "start": "next",
        "build": "next build",
        "postbuild": "next-sitemap --config next-sitemap.config.js",
        "dist": "npm run build && next export",
        "lint": "next lint",
        "prettier": "prettier --write **/*.{json,js,ts,tsx,scss}",
        "lint-staged": "lint-staged",
        "prepare": "is-ci || husky install"
    },
    "lint-staged": {
        "*.{json,js,ts,tsx,scss}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "dayjs": "1.11.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-icons": "4.3.1",
        "react-markdown": "8.0.2"
    },
    "devDependencies": {
        "@types/node": "17.0.23",
        "@types/react": "17.0.39",
        "@types/react-dom": "17.0.11",
        "@typescript-eslint/eslint-plugin": "5.18.0",
        "@typescript-eslint/parser": "5.18.0",
        "eslint": "8.13.0",
        "eslint-config-next": "12.1.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "7.0.4",
        "is-ci": "3.0.1",
        "lint-staged": "12.3.7",
        "next": "12.1.4",
        "next-compose-plugins": "2.2.1",
        "next-sitemap": "2.5.19",
        "prettier": "2.6.2",
        "sass": "1.50.0",
        "typescript": "4.6.3"
    }
}
