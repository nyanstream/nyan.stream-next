{
    "name": "nyan.stream",
    "description": "Source code of NYAN.STREAM project.",
    "private": true,
    "version": "3.0.0-dev",
    "author": "Dmitry <dmitry@cojam.ru> (https://e965.ru)",
    "repository": "github:nyanstream/nyan.stream",
    "license": "MIT",
    "scripts": {
        "start": "next",
        "build": "next build",
        "postbuild": "next-sitemap --config next-sitemap.config.js",
        "dist": "npm run build && next export",
        "lint": "eslint --ext .js,.ts,.tsx",
        "prettier": "prettier --write **/*.{json,js,ts,tsx,scss}",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.{json,js,ts,tsx,scss}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "dependencies": {
        "dayjs": "1.9.7",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-icons": "4.1.0",
        "react-markdown": "^5.0.3"
    },
    "devDependencies": {
        "@types/node": "14.14.14",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@typescript-eslint/eslint-plugin": "4.11.0",
        "@typescript-eslint/parser": "4.11.0",
        "eslint": "7.16.0",
        "eslint-config-prettier": "7.1.0",
        "eslint-plugin-prettier": "3.3.0",
        "eslint-plugin-react": "7.21.5",
        "husky": "4.3.6",
        "lint-staged": "10.5.3",
        "next": "10.0.4",
        "next-compose-plugins": "2.2.1",
        "next-images": "1.6.2",
        "next-sitemap": "1.3.22",
        "prettier": "2.2.1",
        "sass": "1.30.0",
        "typescript": "4.1.3"
    }
}
