{
    "name": "nyan.stream",
    "description": "Source code of NYAN.STREAM website.",
    "private": true,
    "version": "3.0.2",
    "author": "Dmitry <dmitry@cojam.ru> (https://e965.ru)",
    "repository": "github:nyanstream/nyan.stream",
    "license": "MIT",
    "scripts": {
        "start": "next",
        "build": "next build",
        "postbuild": "next-sitemap --config next-sitemap.config.js",
        "dist": "npm run build && next export",
        "lint": "eslint --ext .js,.ts,.tsx",
        "prettier": "prettier --write **/*.{json,js,ts,tsx,scss}",
        "lint-staged": "lint-staged",
        "prepare": "is-ci || husky install"
    },
    "lint-staged": {
        "*.{json,js,ts,tsx,scss}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "dayjs": "1.10.4",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-icons": "4.2.0",
        "react-markdown": "6.0.1"
    },
    "devDependencies": {
        "@types/node": "15.0.2",
        "@types/react": "17.0.5",
        "@types/react-dom": "17.0.3",
        "@typescript-eslint/eslint-plugin": "4.22.1",
        "@typescript-eslint/parser": "4.22.1",
        "eslint": "7.25.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-react": "7.23.2",
        "husky": "6.0.0",
        "is-ci": "3.0.0",
        "lint-staged": "10.5.4",
        "next": "10.2.0",
        "next-compose-plugins": "2.2.1",
        "next-images": "1.7.0",
        "next-sitemap": "1.6.42",
        "prettier": "2.2.1",
        "sass": "1.32.12",
        "typescript": "4.2.4"
    }
}
